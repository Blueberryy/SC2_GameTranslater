<Application x:Class="SC2_GameTranslater.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SC2_GameTranslater"
             StartupUri="SC2_GameTranslater_Window.xaml">
    <Application.Resources>
        <ResourceDictionary x:Name="ApplicatioLanguage">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Fluent;Component/Themes/Generic.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Assets/Assets.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Assets/Style.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>         
    </Application.Resources>
</Application>
