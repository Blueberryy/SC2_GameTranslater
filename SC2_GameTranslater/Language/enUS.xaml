    <sys:String x:Key="TP_RibbonButton_New_Text" xml:space="preserve">Please select a SC2 Map/Mod components (ComponentList.SC2Components) to create a new translated project for the Map/Mod you select.&#x000D;HotKey: Ctrl + N</sys:String>
    <sys:String x:Key="TP_RibbonButton_New_DisableReason">Reason</sys:String>
